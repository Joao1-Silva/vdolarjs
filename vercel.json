{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/dolar",
      "dest": "index.js"
    },
    {
      "src": "/styles.css",
      "dest": "public/styles.css"
    },
    {
      "src": "/app.js",
      "dest": "public/app.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}